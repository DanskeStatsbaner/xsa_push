PROCEDURE "DataWareHouse.Database.Procedures::token"(
		OUT OUTPUT table (
			JWT NVARCHAR(5000)
		)
	)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   READS SQL DATA AS
BEGIN

	CALL "DataWareHouse.Database.VirtualProcedures::FileReaderExec"('/usr/sap/dataprovagent/FileShare/MILIMAT0810/token.sh', '', 8, response);
	OUTPUT = SELECT RESULT AS JWT FROM :response;

END

