PROCEDURE "DataWareHouse.Database.Procedures::bash"(
		IN COMMAND VARCHAR(5000),
		OUT OUTPUT table (
			RESPONSE NVARCHAR(5000)
		)
	)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER AS
BEGIN

	ins = SELECT COMMAND FROM "DataWareHouse.Database.Synonyms::DUMMY";
	CALL "DataWareHouse.Database.Procedures::bash_r"(INS => :ins, OUTS => response);
	
	OUTPUT = SELECT RESPONSE FROM :response;

END