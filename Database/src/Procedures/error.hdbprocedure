PROCEDURE "DataWareHouse.Database.Procedures::error"(IN MESSAGE VARCHAR(5000))
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   READS SQL DATA AS
BEGIN
    DECLARE CUSTOM_ERROR CONDITION FOR SQL_ERROR_CODE 10001;
    DECLARE EXIT HANDLER FOR CUSTOM_ERROR RESIGNAL;
    SIGNAL CUSTOM_ERROR SET MESSAGE_TEXT = MESSAGE;
END